import java.util.*;
public class Solution{
public static void main(String[] args){
    Scanner sc = new Scanner(System.in);
    int N=sc.nextInt();
    int[] al = new int[N];
    for (int i=0;i<N;i++){
        al[i]=sc.nextInt();
    }
    int pos=sc.nextInt();
    int sIndex = pos-1;
    int eIndex=N-pos;
    int temp=al[sIndex];
    al[sIndex]=al[eIndex];
    al[eIndex]=temp;
    for(int i=0;i<N;i++){
        System.out.print(al[i]+" ");
    }
}
}
        
