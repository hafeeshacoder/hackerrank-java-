import java.io.*;
import java.util.*;

class Node
{
    int data;
    Node left, right;

    Node(int data)
    {
        this.data = data;
        this.left = null;
        this.right = null;
    }
}

public class Solution 
{
    // Function to build tree from level-order input
    static Node buildTree(int[] arr)
    {
        if(arr.length == 0)
        {
            return null;
        }

        Node root = new Node(arr[0]);   // root node
        Queue<Node> q = new LinkedList<>();
        q.add(root);

        int i = 1;
        while(i < arr.length)
        {
            Node current = q.poll();

            if(i < arr.length)
            {
                current.left = new Node(arr[i]);
                i++;
                q.add(current.left);
            }

            if(i < arr.length)
            {
                current.right = new Node(arr[i]);
                i++;
                q.add(current.right);
            }
        }

        return root;
    }

    // Function to invert (mirror) the binary tree
    static Node invertTree(Node root)
    {
        if(root == null)
        {
            return null;
        }

        // Swap left and right child
        Node temp = root.left;
        root.left = root.right;
        root.right = temp;

        // Recursively invert subtrees
        invertTree(root.left);
        invertTree(root.right);

        return root;
    }

    // Function to print tree in level order
    static void displayTree(Node root)
    {
        if(root == null)
        {
            return;
        }

        Queue<Node> q = new LinkedList<>();
        q.add(root);

        while(!q.isEmpty())
        {
            Node node = q.poll();
            System.out.print(node.data + " ");

            if(node.left != null)
                q.add(node.left);

            if(node.right != null)
                q.add(node.right);
        }
    }

    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);

        int no = sc.nextInt();
        int[] arr = new int[no];

        for(int i = 0; i < no; i++)
        {
            arr[i] = sc.nextInt();
        }

        Node root = buildTree(arr);     // Build tree
        root = invertTree(root);        // Invert the tree
        displayTree(root);              // Print inverted tree
    }
}
